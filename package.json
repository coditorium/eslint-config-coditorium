{
  "name": "eslint-config-coditorium",
  "version": "1.0.0",
  "description": "Coditorium linter rules for JavaScript.",
  "homepage": "https://bitbucket.org/pmendelski/eslint-config-coditorium#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:coditorium/eslint-config-coditorium.git"
  },
  "scripts": {
    "clean": "rimraf ./config",
    "lint": "eslint ./src",
    "test-successes": "eslint ./test/*/successes",
    "test-failures": "mocha --compilers js:babel-core/register ./test/failures.spec.js",
    "test": "npm run test-successes && npm run test-failures",
    "build": "babel-node ./src/index.js ./config",
    "ci": "npm run clean && npm run lint && npm run build && npm run test"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "yamljs": "^0.2.8"
  },
  "dependencies": {
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0"
  }
}
